{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/layout/Layout.js", "pages/_app.jsx", "pages/layout/Layout.jsx", "pages/layout/Navigation.jsx", "pages/layout/Footer.jsx", "pages/_document.jsx", "pages/SeConnecter.jsx", "pages/index.jsx", "pages/Custom404.jsx", "pages/Emplois.jsx"],
  "exclude": ["node_modules"]
}
